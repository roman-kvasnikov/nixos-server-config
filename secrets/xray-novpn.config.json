{
	"log": {
		"loglevel": "warning"
	},
	"inbounds": [
		{
			"tag": "http",
			"listen": "127.0.0.1",
			"port": 10809,
			"protocol": "http",
			"settings": {
				"allowTransparent": false
			}
		},
		{
			"tag": "socks",
			"listen": "127.0.0.1",
			"port": 10808,
			"protocol": "socks",
			"settings": {
				"auth": "noauth",
				"udp": true
			}
		}
	],
	"outbounds": [
		{
			"tag": "direct",
			"protocol": "freedom",
			"settings": {}
		}
	],
	"routing": {
		"domainStrategy": "IPIfNonMatch",
		"domainMatcher": "hybrid",
		"rules": [
			{
				"type": "field",
				"ip": ["0.0.0.0/0", "::/0"],
				"outboundTag": "direct"
			}
		]
	}
}
